{
  "name": "bookmark-mis",
  "version": "1.0.0",
  "description": "Bookmark Management System - Monorepo",
  "main": "index.js",
  "scripts": {
    "build": "turbo run build",
    "build:backend": "turbo run build --filter=backend",
    "build:dashboard": "turbo run build --filter=dashboard",
    "dev": "turbo run dev",
    "dev:backend": "turbo run dev --filter=backend",
    "dev:dashboard": "turbo run dev --filter=dashboard",
    "start": "turbo run start",
    "start:backend": "turbo run start --filter=backend",
    "start:dashboard": "turbo run start --filter=dashboard",
    "install:all": "pnpm install",
    "migrate": "cd apps/backend && pnpm prisma:migrate",
    "migrate:dev": "cd apps/backend && pnpm prisma:migrate:dev",
    "migrate:reset": "cd apps/backend && pnpm prisma:migrate:reset",
    "prisma:generate": "cd apps/backend && pnpm prisma:generate",
    "seed": "cd apps/backend && pnpm seed",
    "lint": "turbo run lint",
    "type-check": "turbo run type-check",
    "test": "turbo run test"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "packageManager": "pnpm@10.24.0",
  "devDependencies": {
    "autoprefixer": "^10.4.22",
    "postcss": "^8.5.6",
    "tailwindcss": "^4.1.17",
    "turbo": "^2.6.1"
  },
  "dependencies": {
    "@nestjs/common": "^11.1.9",
    "@nestjs/config": "^4.0.2",
    "@nestjs/core": "^11.1.9",
    "@nestjs/jwt": "^11.0.1",
    "@nestjs/passport": "^11.0.5",
    "@nestjs/platform-express": "^11.1.9",
    "@nestjs/typeorm": "^11.0.0",
    "@prisma/client": "^7.0.1",
    "bcryptjs": "^3.0.3",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.3",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "pg": "^8.16.3",
    "pgvector": "^0.2.1",
    "prisma": "^7.0.1",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.2",
    "typeorm": "^0.3.27"
  }
}
